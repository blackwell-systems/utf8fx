        "type": "native",
        "self_closing": true,
        "description": "Visual divider bar with themed colors",
        "contexts": ["block"],
        "args": [],
        "optional_params": {
          "style": {
            "type": "shield_style",
            "default": "flat-square",
            "description": "Badge style (flat, flat-square, for-the-badge, plastic, social)"
          }
        }
      },
      "swatch": {
        "type": "native",
        "self_closing": true,
        "description": "Single color block",
        "contexts": ["inline", "block"],
