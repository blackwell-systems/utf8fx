        "contexts": ["inline", "block"],
        "args": ["logo"],
        "optional_params": {
          "style": {
            "type": "shield_style",
            "default": "flat-square",
            "description": "Badge style"
          }
        }
      },
      "status": {
        "type": "native",
        "self_closing": true,
        "description": "Status indicator block (success, warning, error, info)",
        "contexts": ["inline", "block"],
        "args": ["level"],
        "optional_params": {
          "style": {
            "type": "shield_style",
            "default": "flat-square",
            "description": "Badge style"
          }
        }
      },
      "header": {
        "type": "expand",
        "self_closing": false,
        "description": "Section header with gradient frame and bold text",
        "contexts": ["block"],
        "args": [],
        "template": "{{frame:gradient}}{{mathbold:separator=dot}}$content{{/mathbold}}{{/frame}}"
      },
      "callout": {
        "type": "expand",
        "self_closing": false,
        "description": "Callout box with status indicator",
        "contexts": ["block"],
        "args": ["level"],
        "template": "{{frame:solid-left}}{{shields:block:color=$1:style=flat-square/}} $content{{/frame}}"
      },
      "section": {
        "type": "expand",
        "self_closing": true,
        "description": "Section header with divider (GitHub-optimized)",
        "contexts": ["block"],
        "args": ["title"],
        "template": "## $1\n{{ui:divider/}}"
      },
      "callout-github": {
        "type": "expand",
        "self_closing": false,
        "description": "GitHub-style blockquote callout with status indicator",
        "contexts": ["block"],
        "args": ["type"],
        "template": "{{ui:status:$1/}} **Note**\n$content",
        "post_process": "blockquote"
      },
      "statusitem": {
        "type": "expand",
        "self_closing": true,
        "description": "Inline status indicator with label and text",
        "contexts": ["inline", "block"],
        "args": ["label", "level", "text"],
        "template": "{{ui:status:$2/}} **$1**: $3"
      },
      "row": {
        "type": "native",
        "self_closing": false,
        "description": "Horizontal row of badges with alignment control. Wraps in HTML for GitHub compatibility.",
        "contexts": ["block"],
        "args": [],
        "optional_params": {
          "align": {
            "type": "enum",
            "values": ["left", "center", "right"],
            "default": "center",
            "description": "Horizontal alignment of the row"
          }
        }
      }
    },

